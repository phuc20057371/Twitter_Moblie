{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "lib": [
      "ES6",
      "DOM"
    ],
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
